---
devices:
{% for item in csvfile.split("\n") %}
{%   if loop.index != 1 %}
{%     set list = item.split(",") %}

  - switch: 
      hostname: '{{ list[0]|trim() }}'
      interface: '{{ list[1]|trim() }}'
      interface_ip: '{{ list[2]|trim() }}'
      interface_mask: '{{ list[3]|trim() }}'
      interface_description: '{{ list[4]|trim() }}'
{%   endif %}
{% endfor %}
